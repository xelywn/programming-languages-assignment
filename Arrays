#include <stdio.h>

int main() {
    int arr[100], n = 0;
    arr[n++] = 10;

    for (int i = n; i > 0; i--)
        arr[i] = arr[i - 1];
    arr[0] = 5;
    n++;

    for (int i = 1; i < n - 1; i++)
        arr[i] = arr[i + 1];
    n--;

    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);

    return 0;
}
